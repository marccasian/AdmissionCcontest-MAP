<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.InscrieriController">
   <children>
      <BorderPane prefHeight="655.0" prefWidth="886.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <top>
            <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <ImageView fitHeight="120.0" fitWidth="320.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../imgs/inscrieri.jpg" />
                     </image>
                  </ImageView>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="150.0" prefWidth="550.0" text="Inscrieri" textFill="#236f48">
                     <font>
                        <Font name="System Bold" size="48.0" />
                     </font>
                  </Label>
               </children>
               <BorderPane.margin>
                  <Insets bottom="10.0" left="10.0" top="10.0" />
               </BorderPane.margin>
            </HBox>
         </top>
         <left>
            <HBox spacing="20.0">
               <children>
                  <TableView fx:id="inscrieriTable" prefHeight="480.0" prefWidth="380.0" BorderPane.alignment="CENTER">
                    <columns>
                      <TableColumn fx:id="candidatColumn" prefWidth="200.0" text="Candidat" />
                      <TableColumn fx:id="sectieColumn" prefWidth="178.0" text="Sectie" />
                    </columns>
                     <BorderPane.margin>
                        <Insets left="10.0" />
                     </BorderPane.margin>
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </TableView>
               </children>
            </HBox>
         </left>
         <center>
            <VBox alignment="CENTER" prefHeight="410.0" prefWidth="210.0" spacing="80.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="buttonAddI" mnemonicParsing="false" onAction="#handleAddInscriere" prefHeight="50.0" prefWidth="150.0" text="Add" />
                  <Button fx:id="buttonDelI" mnemonicParsing="false" onAction="#handleDeleteInscriere" prefHeight="50.0" prefWidth="150.0" text="Delete" />
                  <Button fx:id="buttonModifI" mnemonicParsing="false" onAction="#handleUpdateInscriere" prefHeight="50.0" prefWidth="150.0" text="Edit" />
               </children>
               <padding>
                  <Insets bottom="40.0" />
               </padding>
            </VBox>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
